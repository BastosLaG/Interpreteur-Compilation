// Petit programme r√©alisant fibonnacci
int main() {
    int n = 5;
    int first = 0;
    int second = 1; 
    int next;
    int c;

    printf("Fibonacci Series:\n");

    c = 0;
    while (c < n) {
        if (c <= 1){
            next = c;
        }
        else {
            next = first + second;
            first = second;
            second = next;
        }
        printf(next);
        printf("\n");
        c = c + 1;
    }
    return 0;
}